[group=5]
    <div class="header__log">
		<a class="header__sign" href="{registration-link}" >Регистрация</a>
		<button class="header__login" type="button">
		<svg xmlns="http://www.w3.org/2000/svg" width="16" height="26" viewBox="0 0 16 26"><path d="M229.006,131.866v6.868a8,8,0,1,0,15.994,0v-6.868H229.006Zm8.9,7.27,0.452,3.012h-2.714l0.452-3.012a1.857,1.857,0,0,1-.889-1.592,1.795,1.795,0,1,1,3.588,0A1.857,1.857,0,0,1,237.908,139.136Zm-4.27-11.859a3.367,3.367,0,1,1,6.73,0V130h2.714v-2.72a6.082,6.082,0,1,0-12.158,0V130h2.715S233.638,127.277,233.638,127.277Z" transform="translate(-229 -121)"/></svg>Войти
		</button>
	</div>
	<form method="post" class="login" style="padding: 10px 0 20px;">
		<div class="login__group">
			<input type="text" name="login_name" id="login_name">
			<label for="login_name">{login-method}</label>
		</div>
		<div class="login__group">
			<input type="password" name="login_password" id="login_password">
			<label for="login_password">Пароль</label>
			<a href="{lostpassword-link}" class="login__forgot">Забыли?</a>
		</div>
		<div class="login__wrap">
			<button class="login__btn" onclick="submit();" type="submit" title="Submit">Войти<svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 18 18"><path d="M106.09,420a0.966,0.966,0,0,0,.3.71l2.316,2.29h-9.7a0.95,0.95,0,0,0-.708.3,0.932,0.932,0,0,0-.3.7,1.013,1.013,0,0,0,1.011,1h9.7l-2.316,2.3a0.932,0.932,0,0,0-.3.7,1.012,1.012,0,0,0,1.011,1,1.006,1.006,0,0,0,.718-0.29l4.045-4a0.984,0.984,0,0,0,.293-0.71,0.949,0.949,0,0,0-.293-0.7l-4.045-4a0.987,0.987,0,0,0-.718-0.3,0.956,0.956,0,0,0-.708.3,0.932,0.932,0,0,0-.3.7m9.617-4.7a0.989,0.989,0,0,0-.718-0.3h-2.023a0.956,0.956,0,0,0-.708.3,0.934,0.934,0,0,0-.3.7,1.012,1.012,0,0,0,1.011,1h1.011v14h-1.011a0.958,0.958,0,0,0-.708.3,0.937,0.937,0,0,0-.3.7,1.012,1.012,0,0,0,1.011,1h2.023a1.008,1.008,0,0,0,.718-0.29A0.984,0.984,0,0,0,116,432V416A0.95,0.95,0,0,0,115.707,415.3Z" transform="translate(-98 -415)"/></svg></button>
			<div class="login__check">
				<input type="checkbox" id="personal" checked="checked">
				<label for="personal">Запомнить меня</label>
			</div>
		</div>
		<input name="login" type="hidden" id="login" value="submit">
			<div class="login__social">
				<div class="login__social-btns">
					[vk]<a href="{vk_url}" target="_blank"><img src="{THEME}/img/social/vk.png" alt="" /></a>[/vk]
					[odnoklassniki]<a href="{odnoklassniki_url}" target="_blank"><img src="{THEME}/img/social/ok.png" alt="" /></a>[/odnoklassniki]
					[facebook]<a href="{facebook_url}" target="_blank"><img src="{THEME}/img/social/fb.png" alt="" /></a>[/facebook]
					[mailru]<a href="{mailru_url}" target="_blank"><img src="{THEME}/img/social/mail.png" alt="" /></a>[/mailru]
					[google]<a href="{google_url}" target="_blank"><img src="{THEME}/img/social/google.png" alt="" /></a>[/google]
					[yandex]<a href="{yandex_url}" target="_blank"><img src="{THEME}/img/social/yandex.png" alt="" /></a>[/yandex]
				</div>
			</div>
	</form>
[/group]